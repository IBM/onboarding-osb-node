{
  "name": "onboarding-osb-node",
  "version": "1.0.0",
  "description": "Open Service Broker Reference App",
  "private": true,
  "main": "dist/src/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/IBM/onboarding-osb-node.git"
  },
  "keywords": [],
  "author": "Shohel Rana <cse_shohel@yahoo.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "watch": "tsc -w",
    "start": "yarn clean && tsc && node dist/src/app.js",
    "dev": "nodemon src/app.ts",
    "build": "yarn clean && tsc",
    "test": "jest",
    "test:ci": "jest --passWithNoTests",
    "start:dev": "concurrently \"yarn watch\" \"yarn dev\"",
    "start:dist": "node dist/src/app.js",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migrate:generate": "yarn typeorm migration:generate -d ./src/db/data-source.ts -n",
    "migrate": "yarn typeorm migration:run -d ./src/db/data-source.ts",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,css,md}\"",
    "check-types": "tsc --noEmit",
    "prettify": "yarn format",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@types/cors": "^2.8.15",
    "argon2": "^0.40.3",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "js-base64": "^3.7.7",
    "jsonwebtoken": "^9.0.2",
    "memory-cache": "^0.2.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.2.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.2",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.5",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.3.1",
    "supertest": "7.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "resolutions": {
    "rimraf": "^5.0.7",
    "glob": "^10.4.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint --fix",
      "npm run prettify --write"
    ]
  }
}
